<% if (site.data.partners.length){ %>
  <div class="widget-wrap">
    <h3 class="widget-title">合作伙伴 & 赞助商</h3>
    <div class="widget">
      <ul>
        <% site.data.partners.forEach(function(partner){ %>
          <li>
            <a href="<%- partner.url %>"><img src="<%- url_for(partner.img) %>" alt="<%- partner.name %>" class="hola-partner-logo"></a>
          </li>
        <% }) %>
      </ul>
    </div>
  </div>
<% } %>